	<artifactId>cart-web</artifactId>
	<packaging>war</packaging>
	<dependencies>
		<dependency>
			<groupId>cn.solarcat</groupId>
			<artifactId>sso-interface</artifactId>
			<version>0.0.1-SNAPSHOT</version>
